# Copy this file to .env and fill in the values before running docker-compose or docker run
# Event Hubs
EVENTHUBS_CONNECTION_STRING=Endpoint=sb://<namespace>.servicebus.windows.net/;SharedAccessKeyName=<keyname>;SharedAccessKey=<key>
EVENTHUB_NAME=<eventhub-name>

# Kafka
KAFKA_BOOTSTRAP_SERVERS=broker1:9092,broker2:9092
KAFKA_TOPIC=<topic-name>
KAFKA_CONSUMER_GROUP=<consumer-group>

# Application settings
APP_ENV=development
LOG_LEVEL=INFO

# Storage/Other
STORAGE_CONNECTION_STRING=DefaultEndpointsProtocol=https;AccountName=<name>;AccountKey=<key>;EndpointSuffix=core.windows.net

# Azure Functions runtime settings (optional)
FUNCTIONS_WORKER_RUNTIME=python
FUNCTIONS_WORKER_PROCESS_COUNT=1

# Secrets should never be committed. Use a secrets manager in production.
